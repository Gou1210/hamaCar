/* pages/carInfo/carInfo.wxss */page {
  background-color: #f0f4f8;
}
.swiper{
  width: 100%;
  height: 983rpx;
}
.carInfo {
  width: 690rpx;
  /* height: 983rpx; */
  background: #fff;
  border-radius: 50px;
  margin: 0 auto;
  margin-top: 30rpx;
}

.sheet {
  display: flex;
  justify-content: space-between;
  height: 104rpx;
  align-items: center;
  border-bottom: 1rpx solid #ddd;
  width: 611rpx;
  margin: 0 auto;
}

.sheet .caption {
  color: #333333;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  position: relative;
  margin-left: 30rpx;
}

.sheet .caption:after {
  content: '*';
  border-radius: 6rpx;
  position: absolute;
  left: -20rpx;
  top: 50%;
  transform: translateY(-40%);
  color: #EA0101;
}

.info .caption:after {
  background: #FF0000;
}

.type .caption:after {
  background: #0F2D5F;
}

.site .caption:after {
  width: 0;
}

.sheet .gut {
  color: #333333;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  position: relative;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.upCarImg{
  border: none;
  height: 240rpx;
}


.carImg {
  margin-left: 200rpx;
  /* margin-top: 200rpx; */
}
.btnList{
display: flex;
justify-content: space-between;
padding: 0rpx 100rpx 30rpx 100rpx;
}
.isWash{
  border: 1rpx solid #F0D208;
  box-shadow: 1rpx 1rpx 1rpx 1rpx  #F0D208;
}
.submit {
  /* margin-top: 50rpx; */
  width: 150rpx;
  height: 88rpx;
  background: #F0D208;
  border-radius: 25rpx;
  line-height: 88rpx;
  text-align: center;
}
.btnList2{
  justify-content: center;
}
.submit2{
  width: 540rpx;
}
.onlyAdd{
  display: flex;
  justify-content: center;
}