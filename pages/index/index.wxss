}

.sheet .caption {
  color: #333333;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  position: relative;
  margin-left: 30rpx;
}
.right {
  display: flex;
  justify-content: end;
  width: 200rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.sheet .caption:after {
  content: "";
  width: 12rpx;
  height: 12rpx;
  background: #4696eb;
  border-radius: 6rpx;
  position: absolute;
  left: -20rpx;
  top: 50%;
  transform: translateY(-50%);
}

.info .caption:after {
  background: #ff0000;
}

.type .caption:after {
  background: #0f2d5f;
}

.site .caption:after {
  background: #00BB00;
}

.sheet .gut {
