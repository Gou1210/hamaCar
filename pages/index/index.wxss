.spot {
  position: absolute;
  top: 50%;
  left: 50%;
  width: auto;
  height: auto;
  padding: 10rpx 20rpx;
  font-size: 24rpx;
  transform: translateX(-50%) translateY(-100%);
  z-index: 999;
  display: flex;
  flex-flow: column nowrap;
  justify-content: flex-start;
  align-items: center;
}

@keyframes spot-contnet {
  0% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.2);
  }
  100% {
    transform: scale(1);
  }
}
.spot-line {
  height: 90rpx;
  margin-top: -40rpx;
  width: 7rpx;
  background: #009575;
  border-radius: 0 5rpx 5rpx 0;
  z-index: -1;
}
.spot-contnet {
  box-sizing: border-box;
  transition: 0.2s all;
  word-break: break-all;
  width: auto;
  min-width: 50rpx;
  height: auto;
  border-radius: 50rpx;
  padding: 10rpx 25rpx;
  line-height: 30rpx;
  font-size: 25rpx;
  max-width: 350rpx;
  background: #009575;
  color: #009575;
  overflow: hidden;
  max-height: 80rpx;
}

.spot-ani {
  width: 50rpx;
  height: 50rpx;
  /* animation: spot-contnet 1s; */
  animation: 0.7s spot-contnet linear infinite normal;
  display: flex;
  align-items: center;
  justify-content: center;
  max-width: 50rpx;
  max-height: 50rpx;
}
.spot-ani::after {
  content: "";
  width: 20rpx;
  height: 20rpx;
  background: rgba(255, 255, 255, 0.8);
  border-radius: 50%;
  flex-shrink: 0;
}
@keyframes spot-over {
  0% {
    max-height: 50rpx;
  }
  100% {
    color: #fff;
    max-height: 80rpx;
  }
}
.spot-over {
  animation: spot-over 0.2s;
  animation-fill-mode: forwards;
}
