.sheet .gut {
  color: #999;
  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 400;
  position: relative;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.sheet .gut input {
  padding-right: 26rpx;
  text-align: end;
}


