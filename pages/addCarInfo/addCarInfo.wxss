page {
  background-color: #f0f4f8;
}

.carInfo {
  width: 690rpx;
  background: #fff;
  border-radius: 50px;
  margin: 0 auto;
  margin-top: 30rpx;
  padding: 30rpx;
}



.sheet {
  display: flex;
  justify-content: space-between;
  height: 104rpx;
  align-items: center;
  border-bottom: 1rpx solid #ddd;
  width: 611rpx;
  margin: 0 auto;
  position: relative;
}

.sheet .caption {
  color: #333333;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: bold;
  position: relative;
  margin-left: 30rpx;
}

.sheet .caption:after {
  content: '*';
  border-radius: 6rpx;
  position: absolute;
  left: -20rpx;
  top: 50%;
  transform: translateY(-40%);
  color: #EA0101;
}

