.addCar {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.addBox {
  width: 690rpx;
  height: 171rpx;
  padding: 20rpx;
  background: #FFFFFF;
  border-radius: 50rpx;
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  position: relative;
}

  
  .sheet {
    display: flex;
    /* justify-content: space-between; */
    height: 90rpx;
    align-items: center;
    width: 611rpx;
    margin: 0 auto;
  }
  
  .sheet .caption {
    color: #333333;
    font-size: 28rpx;
    font-family: PingFang SC;
    font-weight: bold;
    position: relative;
    margin-left: 30rpx;
    margin-right: 27rpx;
  }
  
  .sheet .caption:after {
    content: '*';
    border-radius: 6rpx;
    position: absolute;
    left: -20rpx;
    top: 50%;
    transform: translateY(-40%);
    color: #EA0101;
  }
  
  .info .caption:after {
    background: #FF0000;
  }
  
  .type .caption:after {
    background: #0F2D5F;
  }
  

  
  .sheet .gut {
    color: #999;
    font-size: 22rpx;
    font-family: PingFang SC;
    font-weight: 400;
    position: relative;
    display: flex;
    justify-content: flex-end;
    align-items: center;
  }

  .btnList{
  display: flex;
  justify-content: space-between;
  padding: 0rpx 100rpx 30rpx 100rpx;
  }
  .submit {
    margin-top: 50rpx;
    width: 150rpx;
    height: 88rpx;
    background: #F0D208;
    border-radius: 25rpx;
    line-height: 88rpx;
    text-align: center;
  }

    .btnList2{
      justify-content: center;
    }
    .submit2{
      width: 540rpx;
    }
    .circle{
      width: 41rpx;
      height: 41rpx;
      background-color:#ddd;
      border-radius: 50%;
      color: #fff;
      text-align: center;
      line-height: 41rpx;
      font-size: 30rpx;
      position: absolute;
      right: 40rpx;
      top: 100rpx;
    }
    .active{
      background-color:#F0D208;
    }
    .locationFloor{
      width: 80rpx;
      height: 60rpx;
      background: #F0F4F8;
      border-radius: 15rpx;
      padding: 0rpx 20rpx;
      font-weight: 400;
      color: #999999;
      margin-right: 10rpx;
      font-size: 22rpx;
      text-align: center;
      line-height: 60rpx;
    }