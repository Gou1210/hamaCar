.tabs{
  width: 565rpx;
  height: 60rpx;
  position: relative;
}
.tabs_title{
  display: flex;
  justify-content: space-between;
  background-color: #F0D208;
}
.title_item{
  text-align: center;
  line-height: 60rpx;
  width: 169rpx;
  height: 60rpx;
  border-radius: 25px;
  background-color: #fff;
  font-size: 28rpx;
  transition: all .7s;
}
.active{
  background: #333333;
  color: #F0D208; 
  /* background: #000; */
  /* box-shadow: 6rpx 6rpx 10rpx 4rpx #333 ; */
}
.logo{
  width: 139rpx;
  height: 130rpx;
  position: absolute;
  left: 50%;
  top: -10rpx;
  transform: translateX(-50%);
}
